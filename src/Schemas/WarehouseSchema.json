{
  "title": "WarehouseSchema",
  "description": "Schema for warehouse",
  "type": "array",
  "properties": {
    "address": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30,
      "pattern": "^[a-zA-Z0-9,.]{3,30}$"
    },
    "capacity": {
      "type": "number",
      "minimum": 0,
      "exclusiveMinimum": true
    }
  },

  "require": ["address", "capacity"]
}